<h1>All Restaurants</h1>

<ul>
  <% @restaurants.each do |restaurant| %>
    <li>
      <%= link_to restaurant.name, restaurant_path(restaurant) %>
      - <%= restaurant.category.capitalize %>
      - <%= restaurant.address %>
    </li>
  <% end %>
</ul>

<%= link_to 'Create a New Restaurant', new_restaurant_path %>
